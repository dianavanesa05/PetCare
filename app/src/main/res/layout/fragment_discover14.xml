<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".discover.Discover14Fragment">

    <RelativeLayout
        android:id="@+id/framer"
        android:layout_width="340dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:id="@+id/mdhelp"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:background="@drawable/mdhelp"
            app:layout_constraintStart_toStartOf="parent" />

        <RelativeLayout
            android:id="@+id/group"
            android:layout_width="124dp"
            android:layout_height="24dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp">

            <ImageView
                android:id="@+id/blogs"
                android:layout_width="42dp"
                android:layout_height="18dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:background="@drawable/blog"/>

            <View
                android:id="@+id/line_14"
                android:layout_width="35dp"
                android:layout_marginTop="22dp"
                android:layout_height="1dp"
                android:layout_marginStart="4dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:background="@drawable/line_14" />

            <TextView
                android:id="@+id/videos"
                android:layout_width="51dp"
                android:layout_height="24dp"
                android:layout_alignParentEnd="true"
                android:text="@string/videos"
                android:textAppearance="@style/videos" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/search"
            android:layout_width="22dp"
            android:layout_height="20dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:background="@drawable/search"
            app:layout_constraintStart_toStartOf="parent" />
    </RelativeLayout>


    <TextView
        android:id="@+id/explore"
        android:layout_width="79dp"
        android:layout_height="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="299dp"
        android:gravity="center_vertical"
        android:text="@string/your_groups"
        android:textAppearance="@style/your_groups"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/framer" />

    <RelativeLayout
        android:id="@+id/frame_26080"
        android:layout_width="344dp"
        android:layout_height="91dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/long_frame_dog"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/explore" />

    <RelativeLayout
        android:id="@+id/feed_cards"
        android:layout_width="328dp"
        android:layout_height="574dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/dog_card"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/frame_26080" />

    <RelativeLayout
        android:id="@+id/navigation_"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <RelativeLayout
            android:id="@+id/segmentsContainer"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/segment_1"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="15dp">

                <RelativeLayout
                    android:id="@+id/icon_contai"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:clipToOutline="true">
                    <RelativeLayout
                        android:id="@+id/checkIcon"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentTop="true">
                        <ImageView
                            android:id="@+id/icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/fichecksquare"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentTop="true"
                            />
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:id="@+id/home"
                    android:layout_width="35dp"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:lineSpacingExtra="5sp"
                    android:text="@string/home"
                    android:textAppearance="@style/home"
                    android:translationY="-2.74sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/segment_2"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_toEndOf="@id/segment_1"
                android:layout_marginEnd="15dp">

                <RelativeLayout
                    android:id="@+id/paw_icon"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    >
                    <RelativeLayout
                        android:id="@+id/paw_container"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentTop="true">
                        <ImageView
                            android:id="@+id/paw_img"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/paw"
                            android:background="@drawable/nav_background"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentTop="true"
                            />
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:id="@+id/paw"
                    android:layout_width="35dp"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:lineSpacingExtra="5sp"
                    android:text="@string/discover"
                    android:textAppearance="@style/discover"
                    android:translationY="-2.74sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/mdaddcircle"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_toRightOf="@id/segment_2"
                android:background="@drawable/mdaddcircle"
                />

            <RelativeLayout
                android:id="@+id/segment_4"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_toRightOf="@id/mdaddcircle"
                android:layout_marginStart="15dp"
                >

                <RelativeLayout
                    android:id="@+id/body_icon"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:clipToOutline="true">
                    <RelativeLayout
                        android:id="@+id/body_container"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentTop="true">
                        <ImageView
                            android:id="@+id/body"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/iobodysharp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentTop="true"
                            />
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:id="@+id/community"
                    android:layout_width="35dp"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:lineSpacingExtra="5sp"
                    android:text="@string/community2"
                    android:textAppearance="@style/community"
                    android:translationY="-2.74sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/segment_5"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_toRightOf="@id/segment_4"
                android:layout_marginStart="15dp">

                <RelativeLayout
                    android:id="@+id/profile_icon"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:clipToOutline="true">
                    <RelativeLayout
                        android:id="@+id/profile_container"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentTop="true">
                        <ImageView
                            android:id="@+id/profile_img"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ioperson"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentTop="true"
                            />
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:id="@+id/profile"
                    android:layout_width="35dp"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:lineSpacingExtra="5sp"
                    android:text="@string/profile"
                    android:textAppearance="@style/profile"
                    android:translationY="-2.74sp" />
            </RelativeLayout>


        </RelativeLayout>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>