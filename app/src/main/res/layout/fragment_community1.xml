<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".onboarding.Onboard10Fragment">

    <RelativeLayout
        android:id="@+id/frame_26080"
        android:layout_width="336dp"
        android:layout_height="54dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <TextView
            android:id="@+id/tracker"
            android:layout_width="114dp"
            android:layout_height="24dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:gravity="center"
            android:text="@string/tracker"
            android:textAppearance="@style/tracker"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <RelativeLayout
            android:id="@+id/ellipse_30"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="297dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="3dp"
            android:layout_marginBottom="9dp"
            android:background="@drawable/ellipse">

            <RelativeLayout
                android:id="@+id/bimessagero"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:background="@drawable/bimessageroundeddots"
                tools:layout_editor_absoluteX="379dp"
                tools:layout_editor_absoluteY="51dp"
                android:layout_marginEnd="3dp"
                android:layout_marginTop="3dp"
                android:layout_marginBottom="3dp"
                android:layout_marginStart="3dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                />
        </RelativeLayout>

    </RelativeLayout>

    <TextView
        android:id="@+id/view_all"
        android:layout_width="57dp"
        android:layout_height="19dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="4dp"
        android:gravity="top"
        android:text="@string/view_all"
        android:textAppearance="@style/view_all"
        app:layout_constraintBottom_toTopOf="@+id/active"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/nearby"
        android:layout_width="56dp"
        android:layout_height="19dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="4dp"
        android:gravity="top"
        android:text="@string/nearby"
        android:textAppearance="@style/nearby"
        app:layout_constraintBottom_toTopOf="@+id/active"
        app:layout_constraintStart_toStartOf="parent" />

    <RelativeLayout
        android:id="@+id/active"
        android:layout_width="440dp"
        android:layout_height="100dp"
        android:layout_marginStart="36dp"
        android:background="@drawable/active"
        app:layout_constraintBottom_toTopOf="@+id/my_friends"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/my_friends"
        android:layout_width="82dp"
        android:layout_height="19dp"
        android:layout_marginBottom="20dp"
        android:gravity="top"
        android:text="@string/my_friends"
        android:textAppearance="@style/my_friends"
        app:layout_constraintBottom_toTopOf="@+id/feed_cards"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="parent" />

    <RelativeLayout
        android:id="@+id/feed_cards"
        android:layout_width="336dp"
        android:layout_height="426dp"
        android:background="@drawable/feed_cards"
        app:layout_constraintBottom_toTopOf="@+id/navigation_"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <RelativeLayout
        android:id="@+id/navigation_"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <RelativeLayout
            android:id="@+id/segmentsContainer"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/segment_1"
                android:layout_width="64dp"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:id="@+id/icon_contai"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:clipToOutline="true">
                        <RelativeLayout
                            android:id="@+id/checkIcon"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentTop="true">
                            <ImageView
                                android:id="@+id/icon"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/fichecksquare"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentTop="true"
                                />
                        </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:id="@+id/home"
                    android:layout_width="35dp"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:lineSpacingExtra="5sp"
                    android:text="@string/home"
                    android:textAppearance="@style/home"
                    android:translationY="-2.74sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/segment_2"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_toRightOf="@id/segment_1">

                <RelativeLayout
                    android:id="@+id/paw_icon"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:clipToOutline="true">
                    <RelativeLayout
                        android:id="@+id/paw_container"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentTop="true">
                        <ImageView
                            android:id="@+id/paw_img"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/paw"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentTop="true"
                            />
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:id="@+id/paw"
                    android:layout_width="35dp"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:lineSpacingExtra="5sp"
                    android:text="@string/discover"
                    android:textAppearance="@style/discover"
                    android:translationY="-2.74sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/mdaddcircle"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_toRightOf="@id/segment_2"
                android:background="@drawable/mdaddcircle"
                />

            <RelativeLayout
                android:id="@+id/segment_4"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_toRightOf="@id/mdaddcircle"
                >

            <RelativeLayout
                android:id="@+id/body_icon"
                android:layout_width="64dp"
                android:layout_height="32dp"
                android:clipToOutline="true">
                <RelativeLayout
                    android:id="@+id/body_container"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentTop="true">
                    <ImageView
                        android:id="@+id/body"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/iobodysharp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentTop="true"
                        />
                </RelativeLayout>
            </RelativeLayout>

            <TextView
                android:id="@+id/community"
                android:layout_width="35dp"
                android:layout_height="20dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:gravity="center"
                android:lineSpacingExtra="5sp"
                android:text="@string/community2"
                android:textAppearance="@style/community"
                android:translationY="-2.74sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/segment_5"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_toRightOf="@id/segment_4">

                <RelativeLayout
                    android:id="@+id/profile_icon"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:clipToOutline="true">
                    <RelativeLayout
                        android:id="@+id/profile_container"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentTop="true">
                        <ImageView
                            android:id="@+id/profile_img"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ioperson"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentTop="true"
                            />
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:id="@+id/profile"
                    android:layout_width="35dp"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="center"
                    android:lineSpacingExtra="5sp"
                    android:text="@string/profile"
                    android:textAppearance="@style/profile"
                    android:translationY="-2.74sp" />
            </RelativeLayout>


        </RelativeLayout>

    </RelativeLayout>




</androidx.constraintlayout.widget.ConstraintLayout>