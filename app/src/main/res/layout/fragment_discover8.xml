<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".discover.Discover7Fragment">

    <RelativeLayout
        android:id="@+id/arrow_back_"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/arrow"
        app:layout_constraintEnd_toStartOf="@+id/examples_me"
        app:layout_constraintHorizontal_bias="0.745"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:id="@+id/examples_me"
        android:layout_width="292dp"
        android:layout_height="64dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/category"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/search_new"/>

    <RelativeLayout
        android:id="@+id/category"
        android:layout_width="329dp"
        android:layout_height="23dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:layout_constraintBottom_toTopOf="@+id/list"
        android:layout_marginBottom="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/expert"
            android:layout_width="47dp"
            android:layout_height="20dp"
            android:text="@string/expert"
            android:textAppearance="@style/expert"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true"
            android:gravity="center"
            />

        <TextView
            android:id="@+id/most_voted"
            android:layout_width="74dp"
            android:layout_height="20dp"
            android:text="@string/most_voted"
            android:gravity="center"
            android:textAppearance="@style/most_voted2"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="120dp"
            android:layout_marginEnd="120dp"
            />

        <TextView
            android:id="@+id/creator"
            android:layout_width="48dp"
            android:layout_height="20dp"
            android:text="@string/creator"
            android:textAppearance="@style/creator2"
            android:gravity="center"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/list"
        android:layout_width="329dp"
        android:layout_height="640dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="150dp">

        <RelativeLayout
            android:id="@+id/video_cards1"
            android:layout_width="161dp"
            android:layout_height="316dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:background="@drawable/video7"/>

        <RelativeLayout
            android:id="@+id/video_cards2"
            android:layout_width="161dp"
            android:layout_height="316dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:background="@drawable/video4"/>

        <RelativeLayout
            android:id="@+id/video_cards3"
            android:layout_width="161dp"
            android:layout_height="316dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:background="@drawable/video5"/>

        <RelativeLayout
            android:id="@+id/video_cards4"
            android:layout_width="161dp"
            android:layout_height="316dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:background="@drawable/video6"/>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>